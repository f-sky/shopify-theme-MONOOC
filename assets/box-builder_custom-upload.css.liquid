#bxp-steps {
    position: relative;
}
#bxp-steps .bxp-step {
    position: relative;
}
#bxp-progress .bxp-progress-bar {
    border-bottom: solid 3px rgba(58,58,58,1);
    margin-bottom: -1px;
}

#bxp-progress #bxp-steps .bxp-step .bxp-circle {
    border-radius: 0%;
}
#bxp-progress .bxp-progress-number {
    font-size: 14px;
    font-weight: 900;
}
#bxp-progress .bxp-progress-title {
    font-size: 14px;
    font-weight: 900;
    max-width: 70%;
    margin: auto;
    text-transform: uppercase;
}
#bxp-steps .bxp-step img {
    position: absolute;
    top: -65px;
    left: 50%;
    transform: translateX(-50%);
}
#bxp-steps .bxp-step:nth-child(1) img {
    content: url({{ 'logo_step_1.png' | asset_img_url: '50x' }});
}
#bxp-steps .bxp-step:nth-child(2) img {
    content: url({{ 'logo_step_2.png' | asset_img_url: '50x' }});
}
#bxp-steps .bxp-step:nth-child(3) img {
    content: url({{ 'logo_step_3.png' | asset_img_url: '50x' }});
}



#bxp-bldr-wizard_container #bxp-bldr-middle-wizard .bxp-step-box {
    border: 3px solid purple;
    background: purple;
}

#bxp-bldr-wizard_container #bxp-bldr-middle-wizard .bxp-step-box .bxp-reset {
    border-radius: 20px;
    background: #ffe86c;
    color: black;
    font-weight: 700;
    border: black 2px solid;
}

#bxp-bldr-wizard_container #bxp-bldr-middle-wizard .bxp-step-box .bxp-step-box-list {
    color: #fff;
}

@media only screen and (max-width:991px){
    #bxp-bldr-wizard_container #bxp-bldr-middle-wizard .bxp-step-box .bxp-step-box-list {
        display: none;
    }
}
#bxp-bldr-middle-wizard .bxp-thumbnail-qty {
    border-radius: 50%;
    width: 20px;
    height: 20px;
    background: #d1baf2;
    border: 2px solid #140030;
    color: #140030;
    display: flex;
    justify-content: center;
    font-size: 13px;
    font-weight: 600;
    padding: 0;
    letter-spacing: 0px;
}


.bxp-modal__container #bxp-modal__content {
    color: #000000 !important;
}

.bxp-modal__container #bxp-modal__content .bxp-modal__title,
.bxp-modal__container #bxp-modal__content #bxp-prod-info-content {
    color: #000000 !important;
}

#bxp-prod-info-content p,
#bxp-prod-info-content span {
    color: #000000 !important;
}
.bxp-bldr-submit.bxp-bldr-wizard-step {
    margin-bottom: 0px;
}
.bxp-bldr-submit.bxp-bldr-wizard-step > div.bxp-bldr-question_title{
    display: none;
}
.bxp-bldr-submit.bxp-bldr-wizard-step .bxp-bldr-box_general {
    visibility: hidden;
    height: 0px;
}
.bxp-bldr-submit.bxp-bldr-wizard-step .bxp-bldr-box_general {
    padding: 0px;
}
.bxp-bldr-submit.bxp-bldr-wizard-step .bxp-step-content > div:last-child {
    height: 0;
}


.custom-bldr_section-hero {
    position: relative;
    background-color: #ffffff;
    overflow-x: hidden;
}

.custom-bldr_section-hero:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: -5%;
    width: 115%;
    height: 15%;
    background-image: url({{ 'wiggle_border_white.png' | asset_img_url: '2048x' }});
    background-size: cover;
    background-repeat: no-repeat;
}

/** -------- Box of Selected Products begin -------- */

#bxp-bldr-middle-wizard .bxp-step-box {
    position: sticky;
    top: 104px;
    z-index: 3;
    background: white;
}
.bxp-summary-container {
    pointer-events: none;
}

.bxp-thumbnail-remove {
    visibility: hidden;
}

.bxp-summary-container:after {
    content: 'X';
    top: 0px;
    position: absolute;
    background-color: rgba(58,58,58,1);
    color: white;
    right: 0;
    font-size: 10px;
    padding: 2px 4px;
    cursor: pointer;
    /* opacity: 0.6; */
    pointer-events: all;

    border-radius: 50%;
    width: 20px;
    height: 20px;
    background: #d1baf2;
    border: 2px solid #140030;
    color: #140030;
    display: flex;
    justify-content: center;
    font-weight: 600;
}

/** -------- Box of Selected Products end -------- */

/** -------- Filter Group start -------- */
.bxp-bldr-styled-select .bxp-styled-select {
    border-radius: 20px;
    border: solid 3px purple;
}
.bxp-bldr-styled-select input.bxp-filter-title {
    border-radius: 20px;
    border: solid 3px purple;
}
/** -------- Filter Group end -------- */

.bxp-step-content .bxp-bldr-item label {
    border: 0;
}

.bxp-step-content .bxp-bldr-item input[type=radio]:not(old):checked+label {
    box-shadow: none;
    border: 0;
}

.bxp-step-content .bxp-bldr-item input[type=radio]:not(old):checked+label:before{
    color: #d1baf2;
    font-size: 2.2rem;
    right: 10px;    /* 5px */
    background: black;
    width: 30px;
    height: 30px;
    line-height: 0.9;
    border-radius: 50%;
    border: 2px solid black;
    box-sizing: content-box;
}

.bxp-bldr-item label .bxp-qty-num {
    border-radius: 50%;
    width: 35px;
    height: 35px;
    background: #d1baf2;
    border: 2px solid #140030;
    color: #140030;
    display: flex;
    justify-content: center;
    font-size: 30px;
    font-weight: 600;
    line-height: 1;
}

.bxp-step-content .bxp-bldr-item label .bxp-bldr-item_price {
    border-radius: 20px;
    background: #ffe86c;
    border: black 2px solid;
    color: black;
    font-weight: 800;

    /* padding-left: 12px;
    padding-right: 12px; */
    padding-left: 5px;
    padding-right: 5px;
}

.bxp-step-content .bxp-bldr-item label .bxp-bldr-item_price .bxp-bldr-add-text {
    color: black;
    font-weight: 900;
}

.bxp-bldr-item input[type=radio]:not(old):checked+label .bxp-bldr-item_price {
    background: #ffba00;
}

label select.bxp-bldr-form-control {
    border-radius: 20px;
    border: 2px solid purple;
}
@media only screen and (max-width:589px){
    label select.bxp-bldr-form-control {
        padding: 5px;
        height: 30px;
        font-size: 12px !important;
        margin-top: 16px;
    }    
}
label .bxp-bldr-qty-down {
    border-radius: 50%;
    /* width: 30px;
    height: 30px; */
    width: 25px;
    height: 25px;
    background: #d1baf2;
    border: 2px solid #140030;
    padding: 0px;
    color: #140030;
    font-size: 39px; /*40px*/
    line-height: 0.4;
    margin-bottom: 0px;
    vertical-align: middle;
    padding-left: 3px;
}


label .bxp-bldr-qty-up {
    border-radius: 50%;
    width: 25px;
    height: 25px;
    background: #d1baf2;
    border: 2px solid #140030;
    padding: 0px;
    color: #140030;
    /* font-size: 36px; */
    font-size: 30px;
    line-height: 0.4;
    margin-bottom: 1px;
    vertical-align: middle;
    padding-top: 6px;
    padding-left: 2px;
}

@media only screen and (max-width:439px){
    label .bxp-bldr-qty-down {
        width: 20px;
        height: 20px;
        font-size: 30px;
        padding-left: 3px;
    }
    label .bxp-bldr-qty-up {
        width: 20px;
        height: 20px;
        font-size: 20px;
        padding-top: 4px;
    }
    .bxp-step-content .bxp-bldr-item label .bxp-bldr-item_price {
        letter-spacing: 1px;
        padding: 2px;
        font-size: 10px;
    }
}
@media only screen and (max-width:320px){
    .bxp-step-content .bxp-bldr-item label .bxp-bldr-item_price {
        letter-spacing: 0px;
    }
    .bxp-step-content .bxp-bldr-item label .bxp-bldr-item_price,
    .bxp-step-content .bxp-bldr-item label .bxp-bldr-item_price .bxp-bldr-add-text {
        font-weight: 700;
    }
}
@media only screen and (max-width:284px){
    label .bxp-bldr-qty-down {
        margin-right: 1px;
    }
    label .bxp-bldr-qty-up {
        margin-left: 1px;
    }
}
button.bxp-deselect {
    border-radius: 50%;
    width: 30px;
    height: 30px;
    background: #d1baf2;
    border: 2px solid #140030;
    padding: 0px;
    color: #140030;

    font-weight: 600;
    margin-top: 16px;
    padding-top: 2px;
    padding-left: 1px;
}

.bxp-bldr-item label .bxp-owl-theme .bxp-owl-dots,
#bxp-modal-img-container .bxp-owl-theme .bxp-owl-dots{
    display: none;
}
.bxp-bldr-item label .bxp-overlay-icon {
    background-size: cover;
    background-position: center;
}

.bxp-step-content .bxp-owl-item:hover .bxp-overlay-icon{
    opacity: 1;
}
.bxp-bldr-item label .bxp-overlay-icon .bxp-icon {
    visibility: hidden;
}

#bxp-modal__content button.bxp-bldr-add {
    border-radius: 20px;
    background: #ffe86c;
    color: black;
    border: black 2px solid;
    font-weight: 700;
}

/** -------- Builder bottom Wizard start -------- */
#bxp-bldr-wizard_container #bxp-bldr-bottom-wizard {
    border-top-color: purple;
}
#bxp-bldr-bottom-wizard button.bxp-bldr-backward {
    border-radius: 20px;
    color: black;
    font-weight: 700;
    border: black 2px solid;
}

#bxp-bldr-bottom-wizard button.bxp-bldr-forward,
#bxp-bldr-bottom-wizard button.bxp-bldr-submit {
    border-radius: 20px;
    background: #ffe86c;
    color: black;
    font-weight: 700;
    border: black 2px solid;
}

/** -------- Builder bottom Wizard end -------- */

#custom-bldr-upload-photo-text {
    margin-top: 30px;
    width: 100%;
}
#custom-bldr-upload-photo-text > div:last-child {
    background-color: #eceefa;
    /* width: 90%; */
    margin: auto;
    padding-bottom: 20px;

    width: 100vw;
    position: relative;
    margin-left: -50vw;
    left: 50%;
}
#custom-bldr-upload-photo-text .custom-bldr-title h3{
    font-size: 1.625rem;
    font-weight: 400;
    text-align: center;
    margin: 10px;
}
#custom-bldr-upload-photo-text .custom-bldr-title p {
    text-align: center;
    color: #a3a3a3;
    font-style: italic;
    font-family: initial;
    font-weight: 700;
}
#custom-bldr-upload-photo-text .wrapper-tiles {
    margin: auto;
    width: 60vw;
    display: grid;
    grid-gap: 0px;
    grid-template-columns: repeat(4, 15vw);
    grid-template-rows: 15vw 15vw 15vw;
    grid-auto-flow: column;

    background-color: #f7f7f7;
    background-position: center;
    background-size: cover;
}

#custom-bldr-upload-photo-text .wrapper-tiles::after {
    content: "";
    width: 20px;
    display: inline-block;
    background-image:url(//cdn.shopify.com/s/files/1/0580/1764/4725/files/right_box.jpg?v=1632131080);
    background-position: center right;
    background-size: 5vw;
    background-repeat: no-repeat;
    height: 45vw;
}

#custom-bldr-upload-photo-text .box {
    /* background-color: #444; */
    color: #fff;
    border-radius: 0px;
    /* border: 1px solid #dcf3ed; */
    padding: 0px;
    margin: 5px;
    font-size: 150%;
}

#custom-bldr-upload-photo-text .box:nth-child(even) {
    /* background-color: #ccc; */
    color: #000;
}

#custom-bldr-upload-photo-text .box.box-custom {
    cursor: pointer;
    padding: 0px;
    margin: 5px;
    overflow-y: hidden;
    border-radius: 0px;
}
#custom-bldr-upload-photo-text .box.box-custom img {
    object-fit: cover;
    max-width: 100%;
    min-height: 100%;

    padding: 2px; /* test */
}

#custom-bldr-upload-photo-text .wrapper-customized-jewelry {
    display: none;
}
#custom-bldr-upload-photo-text .wrapper-customized-jewelry.active {
    display: block;
}

/** */
.wrapper-customized-jewelry {
    width: 60vw;
}
@media only screen and (max-width:991px){
    .wrapper-customized-jewelry {
        width: 100vw;
    }
}
.wrapper-customized-jewelry .customized-jewelry-content {
    display: none;
}
.wrapper-customized-jewelry.active .customized-jewelry-content.active {
    display: block;
}

.wrapper-customized-jewelry.active .customized-jewelry-content .product-info {
    display: flex;
}
.wrapper-customized-jewelry.active {
    margin: auto;
    padding-top: 30px;
}
.wrapper-customized-jewelry.active .customized-jewelry-content .product-info > img,
.wrapper-customized-jewelry.active .customized-jewelry-content .product-info > div {
    flex: 1 1 0;
    width: 0;
    padding: 25px;
}
@media only screen and (max-width:991px){
    .wrapper-customized-jewelry.active .customized-jewelry-content .product-info > img,
    .wrapper-customized-jewelry.active .customized-jewelry-content .product-info > div {
        flex: 1 1 0;
        width: 0;
        padding: 10px;
    }
}
.wrapper-customized-jewelry.active .customized-jewelry-content .product-info div > h3 {
    font-size: 16px;
    margin-bottom: 5px;
    font-weight: 600;
}

.wrapper-customized-jewelry.active .customized-jewelry-content .product-info div.upload-photo {
    margin: auto;
    margin-top: 10%;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 60%;
    height: auto;
    font-size: 14px;

    border-radius: 50%;
    border: 3px #d9e1e6 solid;
    background-color: white;
    cursor: pointer;

    overflow: hidden;
}

.wrapper-customized-jewelry.active .customized-jewelry-content .product-info div.upload-photo:before {
    content: "";
    display: block;
    margin-top: 100%;
}
.wrapper-customized-jewelry.active .customized-jewelry-content .product-info div.upload-photo img {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
}

.wrapper-customized-jewelry.active .customized-jewelry-content .product-info div.upload-photo span {
    color: #a3a3a3;
    font-style: italic;
    font-family: 'fontello';
    font-weight: 600;
    font-size: 12px;
}

.wrapper-giftcard-message {
    display: flex;
    /* max-width: 700px; */
    margin: auto;
    padding-top: 30px;
    background-color: #eceefa;
    width: 60vw;
}
@media only screen and (max-width:991px){
    .wrapper-giftcard-message {
        width: 100vw;
    }
}
.wrapper-giftcard-message > div {
    flex: 1 1 0;
    text-align: center;
    padding: 7px;
}
.wrapper-giftcard-message > div h3 {
    font-size: 18px;
    letter-spacing: unset;
    text-transform: inherit;
    text-align: center;
    font-weight: bold;
}
.wrapper-giftcard-message > div p {
    font-size: 14px;
    font-weight: bolder;
    text-align: center;
   
}
.wrapper-giftcard-message > div:first-child p
{
    padding-top: 15px;
    line-height: 25px;
}
.wrapper-giftcard-message > div:last-child,
.wrapper-giftcard-message > div:last-child p {
    text-align: left;
    padding-top: 5px;
}
.wrapper-giftcard-message > div:last-child p span:last-child {
    padding-left: 10px;
}
.wrapper-giftcard-message > div h4 {
    font-size: 16px;
    letter-spacing: unset;
    text-transform: uppercase;
    margin-top: 20px;
}

.wrapper-giftcard-message hr {
    width: 90px; 
    height: 15px;
    position: relative;
    margin: 0 auto;
    overflow: hidden;
    border: none;
    margin-top: 25px;
    margin-bottom: 10px;
}

.wrapper-giftcard-message hr:before {
    content: "";
    
    background-image: url({{ 'underline-bundle-message.png' | asset_img_url: '100x' }});
    background-repeat: repeat-x;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
}

.wrapper-giftcard-message .wrapper-giftcard-image {
    position: relative;
    width: 75%;
    margin: auto;
}
.wrapper-giftcard-message .wrapper-giftcard-image:after {
    content: "";
    display: block;
    padding-bottom: 100%;
}
.wrapper-giftcard-image img {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
    border-radius: 50%;
}
.wrapper-giftcard-message .wrapper-giftcard-attr_text textarea {
    height: 180px;
    background-color: white;
}
.wrapper-giftcard-attr_from,
.wrapper-giftcard-attr_delivery_to {
    display: flex;
}
.wrapper-giftcard-attr_from input,
.wrapper-giftcard-attr_delivery_to input {
    background-color: white;
    width: 50%;
}
.wrapper-giftcard-attr_from span,
.wrapper-giftcard-attr_delivery_to span {
    font-size: 22px;
    display: flex;
    align-items: center;
    font-family: 'CloisterBlack BT Regular';
    padding-left: 15px;
    font-weight: bold;
    color: #95989f;
}
